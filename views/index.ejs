        <link rel='stylesheet' href='/build/css/style.css' />
        <link rel='stylesheet' href='/build/css/wire.css' />
        <link rel='stylesheet' href='/build/css/dev.css' />

        <script src="/build/js/libs/react.min.js"></script>
        <script src="/build/js/libs/jquery.min.js"></script>
        <script src="/build/js/libs/modernizr-2.8.3-respond-1.4.2.min.js"></script>
      <% } else if (env === "production") { %>
        <link rel='stylesheet' href='/build/prod.css' />
